<!--<h1>タスク一覧</h1>-->
<% if logged_in? %>
<ul>
    <% @tasks.each do |task| %>
    <li><%= link_to task.id, task %> : <%= task.content %> &gt; <%= task.status %></li>
     <% end %>
</ul>
<%= link_to '新規タスクの投稿', new_task_path(@task) %>

<% else %>
  <div class="center jumbotron">
    <div class="text-center">
      <h1>Tasklist</h1>
      <%= link_to 'Sign up now!', signup_path, class: 'btn btn-lg btn-primary' %>
    </div>
  </div>
<% end %>